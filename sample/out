<script>
  $(document).ready(function(){  

    $.ajax({
      type: "GET",
      url: apiLink + 'v1/stock/price-volume', 
      data: {scId: scid},
      dataType: "json",
      beforeSend: function() {      
      },
      success: function(result) {
          let volume_json = JSON.stringify(result.data.stock_price_volume_data);
          volumeGraph(volume_json);
      },error: function (jqXHR, exception) {
          
      },
    });

    function volumeGraph(volume_json){    
    // var volume_json = '{"price":{"1 Week":2.94,"1 Month":8.18,"3 Months":48.77,"YTD":3.67,"1 Year":-10.64,"3 Years":-6.95},"volume":{"Today":{"delivery":0,"cvol":42492092,"cvol_display_text":"4.25 Cr.","delivery_display_text":"","delivery_tooltip_text":"","cvol_tooltip_text":"NSE+BSE Traded Volume : 4.25 Cr."},"Yesterday":{"delivery":5147308,"cvol":28535499,"cvol_display_text":"2.85 Cr.","delivery_display_text":"51.47L (18.04%)","cvol_tooltip_text":"NSE+BSE Traded Volume : 2.85 Cr.","delivery_tooltip_text":"Combined Delivery Volume : 51.47L"},"1 Week":{"delivery":6540129,"cvol":29999780.4,"cvol_display_text":"3.00 Cr.","delivery_display_text":"65.40L (21.80%)","cvol_tooltip_text":"NSE+BSE Traded Volume : 3.00 Cr.","delivery_tooltip_text":"Combined Delivery Volume : 65.40L"},"1 Month":{"delivery":10464466.86,"cvol":40998775.29,"cvol_display_text":"4.10 Cr.","delivery_display_text":"1.05 Cr. (25.52%)","cvol_tooltip_text":"NSE+BSE Traded Volume : 4.10 Cr.","delivery_tooltip_text":"Combined Delivery Volume : 1.05 Cr."}}}';    
    if(volume_json != ''){
    var volume_obj = JSON.parse(volume_json);

      var category_list = []; 
      var cvol_list = []; 
      var delivery_list = []; 
      var delivery_display_text_list = []; 
      var cvol_display_text_list = []; 
      var cvol_tooltip_text_list = []; 
      var delivery_tooltip_text_list = []; 
      
      $.each(volume_obj.volume, function(key, value) {
          
           category_list.push(key);                
           cvol_list.push(value.cvol);                
           delivery_list.push(value.delivery); 
           
           cvol_display_text_list.push(value.cvol_display_text); 
           delivery_display_text_list.push(value.delivery_display_text); 
          
           cvol_tooltip_text_list.push(value.cvol_tooltip_text); 
           delivery_tooltip_text_list.push(value.delivery_tooltip_text); 
                          
                      
     });  

     
      Highcharts.setOptions({
       colors: ['#8693a4', '#8fc7a2'],
       lang: {
            thousandsSep: '',
        }            
   });

      var volume_chart = Highcharts.chart('volume_analysis', {
        chart: {
            type: 'bar',
            height : 280,
            marginRight: 40

        },
        title: {
            text: ''
        }, 
        xAxis: {
            categories: category_list,
            tickLength: 0,
        },
        yAxis: {
            lineWidth:1,
            title: {
                text: 'Volume'
            }
        },
        tooltip: {
           
            formatter: function() {
             
              var display_text = '';
              if(this.series.name == 'Combined Delivery Volume'){

                var cvol = this.y;
                $.each(cvol_list, function(k,v) {                       
                  if(cvol == v){                        
                    display_text = cvol_tooltip_text_list[k];                         
                  }                         
                });

              }else if(this.series.name == 'Delivery'){

                var delivery = this.y;
                $.each(delivery_list, function(k,v) {                       
                  if(delivery == v){                        
                    display_text = delivery_tooltip_text_list[k];                         
                  }                         
                });

              }                
                return display_text;
            }
        },
        plotOptions: {
            bar: {              
                dataLabels: {
                    enabled: true,                                        
                    overflow: 'none',
                    crop: false,
                    color: '#787878'
                  }
                },
                series: {
                    pointPadding: 0,
                    groupPadding: 0.1,
                    borderWidth  : 0,
                }
            },
            legend: {
             enabled : false
         },
         credits: {
            enabled: false
        },
        series: [{
            name: 'Combined Delivery Volume',
            data: cvol_list,
            dataLabels: {             
             formatter: function () {
                  // return ''+ this.y+'Cr.';
                var text = ''; 
                var cvol = this.y;
                $.each(cvol_list, function(k,v) {                       
                  if(cvol == v){                        
                    text = ''+cvol_display_text_list[k];                         
                  }                         
                });
                return text;
             }
            }
        }, {
            name: 'Delivery',
            data: delivery_list,
            dataLabels: {             
             formatter: function () {                 
                var text = ''; 
                var delivery = this.y;
                $.each(delivery_list, function(k,v) {                       
                  if(delivery == v){                        
                    text = ''+delivery_display_text_list[k];                         
                  }                         
                });
                return text;
             }
            }
        }]
    });

     $('#volume_analysis .highcharts-data-labels g').attr('transform', function(i, value) {
            return value.replace(/\d+/, volume_chart.containerWidth - 180);
        });  
  }
}
});
  
</script>
<!--Price And Volume Widget End -->
