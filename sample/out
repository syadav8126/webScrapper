<script>
$(document).ready(function(e) {
    var userAgent = window.navigator.userAgent.toLowerCase();
    var ios = /iphone|ipod|ipad/.test( userAgent );

    if( ios ) {
        isIosRequest = 'Y';
    } else {
        isIosRequest = 'N';
    }

    //new code
        var today = new Date();
        var dd = String(today.getDate()).padStart(2, '0');
        var mm = String(today.getMonth() + 1).padStart(2, '0'); //January is 0!
        var yyyy = today.getFullYear();
        today = dd + '-' + mm + '-' + yyyy;

        if(today < '06-01-2021'){

        	if(showInfoPopup == 'Y'){
                // check cookie here
                
                var d = new Date();
                d.setTime(d.getTime() + (1 * 24 *60 * 60 * 1000));
                var expires = "expires="+ d.toUTCString();
                var infoPopup = getCookie('infoPopup');
                if(infoPopup == ''){  
                    document.cookie = "infoPopup="+today+"; expires="+expires+"; domain=.moneycontrol.com; path=/";
                    $("#pc_features").modal('show');
                } else{
                    if(infoPopup !== today){
                        document.cookie = "infoPopup="+today+"; expires="+expires+"; domain=.moneycontrol.com; path=/";
                        $("#pc_features").modal('show');
                    }
                }
            }
        }
    //end
    // timeInterval()
    preMarTI();
    getExp('futures');
    getExp('options');
});
function preMarTI()
{
    console.log("premarket " + checkTimeSpan("pre"));
	if( checkTimeSpan("pre") ) {
        getPremarketData();
        timerIdp = setInterval( function(){
            if( checkTimeSpan("pre") ) {
                getPremarketData(); 
            }else{
                clearInterval(timerIdp);
                $(".pre_opningtxt").hide();
                timeInterval();
            }
		} , 15000 );
	}else{
        console.log("in else ready"); 
        spotDataUpdate();
        timeInterval()
    }
}
function timeInterval()
{
    timerId = setInterval( function(){
        var d = new Date();
        var hour = d.getHours();
        var minute = d.getMinutes();
        var day_number = d.getDay();
        
        if( checkTimeSpan() ) {
            if(hour != 15)
            {
                t = $("#whichTab").val();
                if(t == "nse" || t == "bse")
                {
                    spotDataUpdate();
                }
                else if(t == "fut")
                {
                    futDataUpdate();
                }
                else if(t == "opt")
                {
                    optDataUpdate();
                }
                $(".ANDBEYOND-DESK-1604").hide();
                $("#andbeyond1604").hide();
                
            }
            else{
                if(minute <= 35)
                {
                    t = $("#whichTab").val();
                    if(t == "nse" || t == "bse")
                    {
                        spotDataUpdate();
                    }
                    else if(t == "fut")
                    {
                        futDataUpdate();
                    }
                    else if(t == "opt")
                    {
                        optDataUpdate();
                    }
                }
                else{
                    clearInterval(timerId);
                }
            }
        }else{
            clearInterval(timerId);
        } 
    }, 5000 );
}
function checkTimeSpan(fr = "")
{
    var d = new Date();
	var hour = d.getHours();
	var minute = d.getMinutes();
	var day_number = d.getDay();
    if(fr == 'pre')
    {
        //console.log("hour "+hour);
         return ((hour == 9 && minute >=0) && (hour == 9 && minute < 15) && day_number != 6 && day_number != 0 );
       //return ((hour <= 18 && minute >=0) && day_number != 6 && day_number != 0 );
    }   
    else{
        if((hour >= 9) && (hour <= 16))
        {
            if((hour == 9 && minute < 15) || (hour == 15 && minute >= 35))
            {
                return false;
            }
            else{
                return true;
            }
        }
        else
        {
            return false;
        }
        // return ((hour >= 9 && minute >= 15) && (hour <= 15 && minute <= 35) && day_number != 6 && day_number != 0 );
    } 
}
</script>
<script>
$(document).ready(function(){
    $(".ANDBEYOND-DESK-1604").hide();
    $("#andbeyond1604").hide();
});
$(window).load(function() {
    $('html, body').animate({
        scrollTop: $(".bottom_nav").offset().top - 20
    }, 500);
});

