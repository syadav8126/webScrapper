</script>
<!-- feedback js start -->
<script>
        function update_visit_count() {
            var visits = $.cookie("visits") || 0;
            visits++;
            var currentDate = new Date();
            var expirationDate = new Date(currentDate.getFullYear(), currentDate.getMonth(), currentDate.getDate()+1, 0, 0, 0);
            $.cookie("visits", visits, { expires : expirationDate });
        }

        //Feedback Get User Deatils
        function getloginUserDetail() {
          var USERNMMC = readCookie("nnmc");
          var token = readCookie("token-normal");
          
          if (USERNMMC != '' && token != '') {

              $.ajax({
                  type: "POST",
                  url: accountsDomain+"user/getdetails",
                  data: {
                      'token': token
                  },
                  dataType: "json",
                  success: function(user_data) {
                      console.log("user fetch success");
                      console.log(user_data);
                        if(user_data.status != 'error'){
                            $("#user_nm").text(user_data['data']['user_fname']+",");
                                                       
                        }else{
                            $("#user_nm").text("Dear User,");                  
                        }                     
                  }
              });
          }else{
              $("#user_nm").text("Dear User,");    
          }  
        }

        function readCookie(cname) {
          var name = cname + "=";
          var ca = document.cookie.split(';');
          for (var i = 0; i < ca.length; i++) {
              var c = ca[i];
              while (c.charAt(0) == ' ') c = c.substring(1);
              if (c.indexOf(name) == 0)
                  return c.substring(name.length, c.length);
          }
          return "";
        }


        $(document).ready(function(){

                    update_visit_count();
                    getloginUserDetail();
                    var close_count = 1;
                    if($.cookie("visits") == 1){

                        var brokerpos = $('#broker_research').offset().top;
                        $(window).scroll(function() {
                            if ($(window).scrollTop() > brokerpos) {
                              $('.pc_feedback').fadeIn();
                              if(close_count == 1){
                                $('.feebackmsg').addClass('open');  
                              }
                            }
                            else {
                               $('.pc_feedback').fadeOut();
                               $('.feebackmsg').removeClass('open');
                            }
                        });

                    }else if($.cookie("visits") == 2){
                        $('.pc_feedback').hide();
                    }else if($.cookie("visits") == 3){
                        $(window).scroll(function() {
                            if ($(window).scrollTop() > 700) {
                              $('.pc_feedback').fadeIn();
                              
                              if(close_count == 1){
                                $('.feebackmsg').addClass('open');  
                              }

                            }
                            else {
                               $('.pc_feedback').fadeOut();
                               $('.feebackmsg').removeClass('open');
                            }
                        });
                    }else if($.cookie("visits") > 3){
                        $(window).scroll(function() {
                            if ($(window).scrollTop() > 700) {
                              $('.pc_feedback').fadeIn();
                             }
                            else {
                               $('.pc_feedback').fadeOut();                               
                            }
                        });
                    }
                    
                    $('.feedico').click(function(){
                        $('.feebackmsg').toggleClass('open')    
                    });
            
            
            
                    $('.feedcheckbx li').on('click', function() {
                     if ($(this).hasClass('selected')) {
                       $(this).removeClass('selected');
                        $(this).find('input[type=checkbox]').removeAttr('checked'); 
                      }
                      else
                      {
                       $(this).addClass('selected');
                          $(this).find('input[type=checkbox]').attr('checked','checked'); 
                      }
                    });
            
                    $('#close_modal').click(function(){
                        $("#display_loader").hide();
                        $('#frm_feedback').trigger("reset");
                        $('#frm_feedback').find("input[type=checkbox]").removeAttr('checked');
                        $('.feedcheckbx li').removeClass('selected');
                        $("#error_message").hide();
                        $('.feedback_thank').hide();
                        $('.feedback_frm').show();
                    });
                    
                    $('.closfeedbx').click(function(){
                        close_count++;
                        $(this).closest('.feebackmsg').removeClass('open');
                    });
                    
                    $('#cancel_btn').click(function(){
                        $("#display_loader").hide();
                        $('#frm_feedback').trigger("reset");
                        $('#frm_feedback').find("input[type=checkbox]").removeAttr('checked');
                        $('.feedcheckbx li').removeClass('selected');
                        $("#error_message").hide();
                        $('.feedback_thank').hide();
                        $('.feedback_frm').show();
                    });
            
        });
    </script>
<!-- feedback js end -->
</body>
</html>

