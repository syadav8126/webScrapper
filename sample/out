<script>	
	function showTrendGraph(trend_title){		
		var trend_jsn = '{"Promoter":{"Sep 2019":{"Holding":57.88,"Pledge":0},"Dec 2019":{"Holding":57.68,"Pledge":0},"Mar 2020":{"Holding":57.63,"Pledge":0},"Jun 2020":{"Holding":57.64,"Pledge":0},"Sep 2020":{"Holding":57.63,"Pledge":0}},"FII":{"Sep 2019":{"Holding":10.83},"Dec 2019":{"Holding":10.98},"Mar 2020":{"Holding":9.59},"Jun 2020":{"Holding":7.91},"Sep 2020":{"Holding":7.75}},"DII":{"Sep 2019":{"Holding":24.41},"Dec 2019":{"Holding":24.55},"Mar 2020":{"Holding":24.67},"Jun 2020":{"Holding":24.9},"Sep 2020":{"Holding":25.49}},"Public":{"Sep 2019":{"Holding":6.89},"Dec 2019":{"Holding":6.77},"Mar 2020":{"Holding":8.12},"Jun 2020":{"Holding":9.55},"Sep 2020":{"Holding":9.12}},"Others":{"Sep 2019":{"Holding":0},"Dec 2019":{"Holding":0.02},"Mar 2020":{"Holding":0},"Jun 2020":{"Holding":0},"Sep 2020":{"Holding":0.01}}}';
		var trend_obj = JSON.parse(trend_jsn);
		var category_list = [];
		var holding_list = [];
		var pledge_list = [];		
		var series_list = [];

		$.each(trend_obj, function(k, v) {			
			if(k == trend_title){
				$.each(v, function(key, value) {
					category_list.push(key);                
		           	holding_list.push(parseFloat(value.Holding)); 
		           	if(trend_title == 'Promoter'){             
		           		pledge_list.push(parseFloat(value.Pledge));  
		           	}  
		        })			
			}
		});

		if(trend_title == 'Promoter'){

			series_list.push({'name' : 'Holding(%)', 'data': holding_list},{'name' : 'Pledges as % of promoter shares(%)', 'data': pledge_list});
		}else{
			series_list.push({'name' : 'Holding(%)', 'data': holding_list});
		}	

		//console.log("series_list", series_list);		
		if(holding_list.length > 0){

				Highcharts.setOptions({
			       colors: ['#8693a4', '#8fc7a2']            
			   });

		      var myChart = Highcharts.chart(trend_title, {
		        chart: {
		            type: 'column',
		            height : 255
		        },
		        title: {
		            text: ''
		        }, 
		        xAxis: {
		            categories: category_list,
		             tickLength: 0,
		        },        
		        yAxis: {
		            title: {
		                text: ''
		            }
		        }, 
		        legend: {
		        	align: 'center',
			        verticalAlign: 'top',
			        x: 0,
			        y: 0,
			        symbolWidth: 20,
				    symbolRadius: 0,
				    squareSymbol: false
			    },  
		         credits: {
		            enabled: false
		        },
		        plotOptions: {
		            column: {
		            	
		                dataLabels: {
		                    enabled: true,
		                    crop: false,
                			overflow: 'none',
		                    color : '#787878',
		                    formatter: function () {                       
		                        return ''+this.y;
		                    }
		                },		                
		                events: {
		                    legendItemClick: function (e) {
		                        e.preventDefault();
		                    }
		                }			                           
		            },
		            series: {
		            		maxPointWidth: 36,
		                    pointPadding: 0,
		                    groupPadding: 0.1,                   
		                }
		        },
		        series: series_list		       
		    });
		}else{				
			var show_data = "";
			show_data = show_data + "<p class='no-data-text'>No Data For "+trend_title+".</p>";
			$("#"+trend_title).html(show_data);
		}
	}
	
	$("document").ready(function() {

		//new code
		var trend_jsn = '{"Promoter":{"Sep 2019":{"Holding":57.88,"Pledge":0},"Dec 2019":{"Holding":57.68,"Pledge":0},"Mar 2020":{"Holding":57.63,"Pledge":0},"Jun 2020":{"Holding":57.64,"Pledge":0},"Sep 2020":{"Holding":57.63,"Pledge":0}},"FII":{"Sep 2019":{"Holding":10.83},"Dec 2019":{"Holding":10.98},"Mar 2020":{"Holding":9.59},"Jun 2020":{"Holding":7.91},"Sep 2020":{"Holding":7.75}},"DII":{"Sep 2019":{"Holding":24.41},"Dec 2019":{"Holding":24.55},"Mar 2020":{"Holding":24.67},"Jun 2020":{"Holding":24.9},"Sep 2020":{"Holding":25.49}},"Public":{"Sep 2019":{"Holding":6.89},"Dec 2019":{"Holding":6.77},"Mar 2020":{"Holding":8.12},"Jun 2020":{"Holding":9.55},"Sep 2020":{"Holding":9.12}},"Others":{"Sep 2019":{"Holding":0},"Dec 2019":{"Holding":0.02},"Mar 2020":{"Holding":0},"Jun 2020":{"Holding":0},"Sep 2020":{"Holding":0.01}}}';
		var trend_obj = JSON.parse(trend_jsn);
		var promoter_holding_list = [];
		var pledge_list = [];		
		var fii_holding_list = [];
		var dii_holding_list = [];
		var public_holding_list = [];
		var others_holding_list = [];
		
		$.each(trend_obj, function(k, v) {			
			if(k == 'Promoter'){
				$.each(v, function(key, value) {					             
		           	promoter_holding_list.push(parseFloat(value.Holding)); 
		           	pledge_list.push(parseFloat(value.Pledge));		           	 
		        })			
			}

			if(k == 'FII'){
				$.each(v, function(key, value) {					             
		           	fii_holding_list.push(parseFloat(value.Holding)); 		           	 
		        })			
			}

			if(k == 'DII'){
				$.each(v, function(key, value) {					             
		           	dii_holding_list.push(parseFloat(value.Holding)); 		           	 
		        })			
			}

			if(k == 'Public'){
				$.each(v, function(key, value) {					             
		           	public_holding_list.push(parseFloat(value.Holding)); 		           	 
		        })			
			}

			if(k == 'Others'){
				$.each(v, function(key, value) {					             
		           	others_holding_list.push(parseFloat(value.Holding)); 		           	 
		        })			
			}
		});

		//
		var dis = 1;
		if(new Set(promoter_holding_list).size == 1 && (promoter_holding_list[0] == 0)){
			$('#li_promoter').hide();
			$('#li_promoter').removeClass('active');
			$("#promoter_tb").attr("aria-expanded","false");
			$('#sh_promo').removeClass('active in');
		}else{
			dis++;
			showTrendGraph('Promoter');
		}
		
		if(new Set(fii_holding_list).size == 1 && (fii_holding_list[0] == 0)){
			$('#li_fii').hide();			
		}else{
			if(dis == 1){
			   dis++;			   
			   showTrendGraph('FII');
			   $('#li_fii').addClass('active');
			   $("#fii_tb").attr("aria-expanded","true");
			   $('#sh_fii').addClass('active in');
			}
		}		

		if(new Set(dii_holding_list).size == 1 && (dii_holding_list[0] == 0)){
			$('#li_dii').hide();			
		}else{
			if(dis == 1){
			   dis++;			   
			   showTrendGraph('DII');
			   $('#li_dii').addClass('active');
			   $("#dii_tb").attr("aria-expanded","true");
			   $('#sh_dii').addClass('active in');
			}
		}

		if(new Set(public_holding_list).size == 1 && (public_holding_list[0] == 0)){
			$('#li_public').hide();			
		}else{
			if(dis == 1){
			   dis++;			   
			   showTrendGraph('Public');
			   	$('#li_public').addClass('active');
				$("#public_tb").attr("aria-expanded","true");
			   	$('#sh_public').addClass('active in');
			}
		}		

		if(new Set(others_holding_list).size == 1 && (others_holding_list[0] == 0)){
			$('#li_others').hide();
		}else{
			if(dis == 1){
			   dis++;			   
			   showTrendGraph('Others');
			   $('#li_others').addClass('active');
			   $("#others_tb").attr("aria-expanded","true");
			   $('#sh_others').addClass('active in');
			}
		}
		//end		
	});
</script>
<!-- trend.html end -->
</div>
