<script>
	$(document).ready(function(){
		var summary_jsn = '{"Promoter":57.63,"FII":7.75,"DII":25.49,"Public":9.12,"others":0.01}';
		var summary_obj = JSON.parse(summary_jsn);

		if(summary_obj != null){
			var summary = [];
			$.each(summary_obj, function(k, v) {
				if(v != 0){					
		          summary.push({name : k, y : v},);              
				}		
		     });	

			 Highcharts.setOptions({
		       colors: ['#1a76c3','#8693a6','#3aaa92','#f2c804','#e67f22']            
		  	 });

			  var myChart = Highcharts.chart('summary_pie_chart', {
			  chart: {
			    type: 'pie',
			    height : 280
			  },

			  title: {
			    text: ''
			  },
			  legend: {
			     layout: 'vertical',
			     align: 'right',
			     verticalAlign: 'middle',
			     padding : 0,		   
			     color: '#787878',
			   },
		      plotOptions: {
		      pie: {     
		      	innerSize: '60%',   
		        showInLegend: true,
		        dataLabels: {
	            	enabled: true, 
	            	allowOverlap:false,
	                distance : 4,
	                padding: 0,			  
	                color: '#787878',			        
	                formatter: function () {
	                    return ''+ this.y +'%';
	                }
	            },
	             point: {
	                events: {
	                    legendItemClick: function (e) {
	                        e.preventDefault();
	                    }
	                }
	            }
		      }
		    },

		      credits: {
		        enabled: false
		   	 },

			  series: [{
			  	name: 'Summary',
			    data : summary
			  }]
			});
		}	
	});
</script>
