<script>
$( document ).ready(function() { 
    updateTechnicals("d","SBI",'N');
});

$('.check_selectbox_element').click(function(event){
	event.stopPropagation();
	$('.check_selectbox_list').toggle();
});	

$(document).click( function(){
    $('.check_selectbox_list').hide();
	$('.graph_selectbox').removeClass("active");
});

$(".graph_selectbox").click(function(){
	$(this).toggleClass("active");
});

function historyPopup(url){
  checkedstring = "";
  $("input:checkbox[name=histCheck]:checked").each(function () {  
      checkedstring = checkedstring+'&'+$(this).attr("id")+"=1";      
  });
  var histDur = $("#historyUl li.active").attr('data-id');  
  var newTabUrl = url+checkedstring+"&dur="+histDur;
  window.open(newTabUrl,'_blank');
}
/*$(".historyDur").click(function(){
  $("#expbutton").attr("href", href+checkedstring);
});*/

$('.graph_top_container .nav-tabs li').click(function() {
    $(this).addClass('active').siblings().removeClass('active');
});
</script>
